{
  "name": "Stay Focused",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This app will encourage productivity",
  "icons": {
  },
  "omnibox": {
    "keyword": "Stay Focused"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["background.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
